{
  "name": "spotify-api-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "npm@11.0.0",
  "scripts": {
    "api-dev": "source venv/bin/activate && python api/app.py",
    "api-start": "source venv/bin/activate && python api/app.py && export FLASK_APP=api/app.py && flask run",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run api-dev\"",
    "generate-venv": "python3 -m venv venv/",
    "build": "next build",
    "start": "next start && api-dev",
    "clean": "rm -rf .next && rm -rf node_modules && rm -rf package-lock.json && npm i",
    "install-api": "python3 -m venv venv/ && source venv/bin/activate && python3 -m pip install -r requirements.txt"
  },
  "keywords": [],
  "author": {
    "name": "MJ Anglin",
    "email": "contact@mjanglin.com",
    "url": "https://www.mjanglin.com",
    "github": "https://github.com/clxrityy"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.0.6",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.6",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.475.0",
    "next": "^15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.1"
  }
}
